<div class="modal-container">
    <div class="modal-header">
        <a class="close" data-dismiss="modal"><span class="glyphicon glyphicon-remove-2"></span></a>
        <div class="modal-title">{{{title}}}</div>
    </div>
    <div class="modal-body">
        <div class="credits">
            {{#each credits}}
                <span>{{entry}}</span></br>
            {{/each}}
        </div>
    </div>
</div>


